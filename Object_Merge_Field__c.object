<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Keep_Most_Least_Recent_Value</fullName>
        <active>true</active>
        <description>Ensures that both the Keep Least Recent Value and Keep Most Recent Value checkboxes are not checked.</description>
        <errorConditionFormula>Keep_Least_Recent_Value__c &amp;&amp; Keep_Most_Recent_Value__c</errorConditionFormula>
        <errorMessage>Both the Keep Least Recent Value checkbox and Keep Most Recent Value checkbox cannot be checked at the same time.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
